{
  "name": "dev-meet",
  "version": "1.0.0",
  "description": "me and norma's final project",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd ./client && npm start",
    "seed": "node scripts/seedDB.js",
    "install": "cd ./client && npm install",
    "build": "cd ./client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DennyDaVjncj/dev-meet.git"
  },
  "author": "Denny da Vjncj & Norma Cruz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DennyDaVjncj/dev-meet/issues"
  },
  "homepage": "https://github.com/DennyDaVjncj/dev-meet#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fontsource-roboto": "^4.0.0",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-tinder-card": "^1.3.1",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
}
